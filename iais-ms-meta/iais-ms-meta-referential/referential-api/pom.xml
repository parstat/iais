	<modelVersion>4.0.0</modelVersion>

	<artifactId>referential-api</artifactId>
	<groupId>com.nbs.iais.ms.meta.referential.api</groupId>

	<dependencies>
		<dependency>
			<groupId>org.springframework.boot</groupId>
